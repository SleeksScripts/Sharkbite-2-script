function tpShark()
    while getgenv().tpShark == true do
        wait()
        for i1, v1 in pairs(game:GetService(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({9135,11655,11970,11235,12075,11760,10185,10395,10605})).Sharks:GetChildren()) do
            for i2, v2 in pairs(v1:GetChildren()) do
                for i3, v3 in pairs(v2:GetChildren()) do
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v3.CFrame
                    print(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({8820,10605,12075,12180,11025,11550,10815}))
                end
            end
        end
    end
end

function fly()
    if getgenv().fly == true then
        repeat
            wait()
        until game.Players.LocalPlayer and game.Players.LocalPlayer.Character and
            game.Players.LocalPlayer.Character:findFirstChild(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180})) and
            game.Players.LocalPlayer.Character:findFirstChild(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({7560,12285,11445,10185,11550,11655,11025,10500}))
        local mouse = game.Players.LocalPlayer:GetMouse()
        repeat
            wait()
        until mouse
        local plr = game.Players.LocalPlayer
        local torso = plr.Character.HumanoidRootPart
        local flying = true
        local deb = true
        local ctrl = {f = 0, b = 0, l = 0, r = 0}
        local lastctrl = {f = 0, b = 0, l = 0, r = 0}
        local maxspeed = 500
        local speed = 0

        function Fly()
            local bg = Instance.new(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({6930,11655,10500,12705,7455,12705,11970,11655}), torso)
            bg.P = 9e4
            bg.maxTorque = Vector3.new(9e9, 9e9, 9e9)
            bg.cframe = torso.CFrame
            local bv = Instance.new(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({6930,11655,10500,12705,9030,10605,11340,11655,10395,11025,12180,12705}), torso)
            bv.velocity = Vector3.new(0, 0.1, 0)
            bv.maxForce = Vector3.new(9e9, 9e9, 9e9)
            repeat
                wait()
                plr.Character.Humanoid.PlatformStand = true
                if ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0 then
                    speed = speed + .5 + (speed / maxspeed)
                    if speed > maxspeed then
                        speed = maxspeed
                    end
                elseif not (ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0) and speed ~= 0 then
                    speed = speed - 1
                    if speed < 0 then
                        speed = 0
                    end
                end
                if (ctrl.l + ctrl.r) ~= 0 or (ctrl.f + ctrl.b) ~= 0 then
                    bv.velocity =
                        ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (ctrl.f + ctrl.b)) +
                        ((game.Workspace.CurrentCamera.CoordinateFrame *
                            CFrame.new(ctrl.l + ctrl.r, (ctrl.f + ctrl.b) * .2, 0).p) -
                            game.Workspace.CurrentCamera.CoordinateFrame.p)) *
                        speed
                    lastctrl = {f = ctrl.f, b = ctrl.b, l = ctrl.l, r = ctrl.r}
                elseif (ctrl.l + ctrl.r) == 0 and (ctrl.f + ctrl.b) == 0 and speed ~= 0 then
                    bv.velocity =
                        ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (lastctrl.f + lastctrl.b)) +
                        ((game.Workspace.CurrentCamera.CoordinateFrame *
                            CFrame.new(lastctrl.l + lastctrl.r, (lastctrl.f + lastctrl.b) * .2, 0).p) -
                            game.Workspace.CurrentCamera.CoordinateFrame.p)) *
                        speed
                else
                    bv.velocity = Vector3.new(0, 0.1, 0)
                end
                bg.cframe =
                    game.Workspace.CurrentCamera.CoordinateFrame *
                    CFrame.Angles(-math.rad((ctrl.f + ctrl.b) * 50 * speed / maxspeed), 0, 0)
            until not flying
            ctrl = {f = 0, b = 0, l = 0, r = 0}
            lastctrl = {f = 0, b = 0, l = 0, r = 0}
            speed = 0
            bg:Destroy()
            bv:Destroy()
            plr.Character.Humanoid.PlatformStand = false
        end
        mouse.KeyDown:connect(
            function(key)
                if key:lower() == VIIRFZvdcJArasLjbxSWTBWFnuTwAC({11760}) then
                    if flying then
                        flying = false
                    else
                        flying = true
                        Fly()
                    end
                elseif key:lower() == VIIRFZvdcJArasLjbxSWTBWFnuTwAC({12495}) then
                    ctrl.f = 1
                elseif key:lower() == VIIRFZvdcJArasLjbxSWTBWFnuTwAC({12075}) then
                    ctrl.b = -1
                elseif key:lower() == VIIRFZvdcJArasLjbxSWTBWFnuTwAC({10185}) then
                    ctrl.l = -1
                elseif key:lower() == VIIRFZvdcJArasLjbxSWTBWFnuTwAC({10500}) then
                    ctrl.r = 1
                end
            end
        )
        mouse.KeyUp:connect(
            function(key)
                if key:lower() == VIIRFZvdcJArasLjbxSWTBWFnuTwAC({12495}) then
                    ctrl.f = 0
                elseif key:lower() == VIIRFZvdcJArasLjbxSWTBWFnuTwAC({12075}) then
                    ctrl.b = 0
                elseif key:lower() == VIIRFZvdcJArasLjbxSWTBWFnuTwAC({10185}) then
                    ctrl.l = 0
                elseif key:lower() == VIIRFZvdcJArasLjbxSWTBWFnuTwAC({10500}) then
                    ctrl.r = 0
                end
            end
        )
        Fly()
    end
end

function InfiniteAmmo()
    while getgenv().InfiniteAmmo == true do
        local aux = loadstring(game:HttpGetAsync(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({10920,12180,12180,11760,12075,6090,4935,4935,11970,10185,12495,4830,10815,11025,12180,10920,12285,10290,12285,12075,10605,11970,10395,11655,11550,12180,10605,11550,12180,4830,10395,11655,11445,4935,8925,11760,10290,11655,11340,12180,4935,7560,12705,10500,11970,11655,12600,11025,10500,10605,4935,11970,10605,12390,11025,12075,11025,11655,11550,4935,11655,10920,10185,12285,12600,4830,11340,12285,10185})))()
        wait()

        

        local scriptPath =
            game:GetService(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({8400,11340,10185,12705,10605,11970,12075})).LocalPlayer.PlayerScripts.ProjectilesClient.WeaponScript.BulletCountModule
        local closureName = VIIRFZvdcJArasLjbxSWTBWFnuTwAC({11970,10605,12075,10605,12180,9135,10605,10185,11760,11655,11550,12075})
        local upvalueIndex = 3
        local closureConstants = {}

        local closure = aux.searchClosure(scriptPath, closureName, upvalueIndex, closureConstants)
        local value = 10000

        -- DO NOT RELY ON THIS FEATURE TO PRODUCE 100% FUNCTIONAL SCRIPTS
        debug.setupvalue(closure, upvalueIndex, value)
    end
end


