                function chamEsp()
        local dwEntities = game:GetService(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({8400,11340,10185,12705,10605,11970,12075}))
local dwLocalPlayer = dwEntities.LocalPlayer 
local dwRunService = game:GetService(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({8610,12285,11550,8715,10605,11970,12390,11025,10395,10605}))

local settings_tbl = {
    ESP_Enabled = true,
    ESP_TeamCheck = false,
    Chams = true,
    Chams_Color = Color3.fromRGB(0,0,255),
    Chams_Transparency = 0.1,
    Chams_Glow_Color = Color3.fromRGB(255,0,0)
}

function destroy_chams(char)

    for k,v in next, char:GetChildren() do 

        if v:IsA(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({6930,10185,12075,10605,8400,10185,11970,12180})) and v.Transparency ~= 1 then

            if v:FindFirstChild(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({7455,11340,11655,12495})) and 
            v:FindFirstChild(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({7035,10920,10185,11445,12075})) then

                v.Glow:Destroy()
                v.Chams:Destroy() 

            end 

        end 

    end 

end

dwRunService.Heartbeat:Connect(function()

    if settings_tbl.ESP_Enabled then

        for k,v in next, dwEntities:GetPlayers() do 

            if v ~= dwLocalPlayer then

                if v.Character and
                v.Character:FindFirstChild(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180})) and 
                v.Character:FindFirstChild(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({7560,12285,11445,10185,11550,11655,11025,10500})) and 
                v.Character:FindFirstChild(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({7560,12285,11445,10185,11550,11655,11025,10500})).Health ~= 0 then

                    if settings_tbl.ESP_TeamCheck == false then

                        local char = v.Character 

                        for k,b in next, char:GetChildren() do 

                            if b:IsA(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({6930,10185,12075,10605,8400,10185,11970,12180})) and 
                            b.Transparency ~= 1 then
                                
                                if settings_tbl.Chams then

                                    if not b:FindFirstChild(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({7455,11340,11655,12495})) and
                                    not b:FindFirstChild(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({7035,10920,10185,11445,12075})) then

                                        local chams_box = Instance.new(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({6930,11655,12600,7560,10185,11550,10500,11340,10605,6825,10500,11655,11970,11550,11445,10605,11550,12180}), b)
                                        chams_box.Name = VIIRFZvdcJArasLjbxSWTBWFnuTwAC({7035,10920,10185,11445,12075})
                                        chams_box.AlwaysOnTop = true 
                                        chams_box.ZIndex = 4 
                                        chams_box.Adornee = b 
                                        chams_box.Color3 = settings_tbl.Chams_Color
                                        chams_box.Transparency = settings_tbl.Chams_Transparency
                                        chams_box.Size = b.Size + Vector3.new(0.02, 0.02, 0.02)

                                        local glow_box = Instance.new(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({6930,11655,12600,7560,10185,11550,10500,11340,10605,6825,10500,11655,11970,11550,11445,10605,11550,12180}), b)
                                        glow_box.Name = VIIRFZvdcJArasLjbxSWTBWFnuTwAC({7455,11340,11655,12495})
                                        glow_box.AlwaysOnTop = false 
                                        glow_box.ZIndex = 3 
                                        glow_box.Adornee = b 
                                        glow_box.Color3 = settings_tbl.Chams_Glow_Color
                                        glow_box.Size = chams_box.Size + Vector3.new(0.13, 0.13, 0.13)

                                    end

                                else

                                    destroy_chams(char)

                                end
                            
                            end

                        end

                    else

                        if v.Team == dwLocalPlayer.Team then
                            destroy_chams(v.Character)
                        end

                    end

                else

                    destroy_chams(v.Character)

                end

            end

        end

    else 

        for k,v in next, dwEntities:GetPlayers() do 

            if v ~= dwLocalPlayer and 
            v.Character and 
            v.Character:FindFirstChild(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180})) and 
            v.Character:FindFirstChild(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({7560,12285,11445,10185,11550,11655,11025,10500})) and 
            v.Character:FindFirstChild(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({7560,12285,11445,10185,11550,11655,11025,10500})).Health ~= 0 then
                
                destroy_chams(v.Character)

            end

        end

    end

end)
end

--CHAM ESP
function chamEsp()
    local dwEntities = game:GetService(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({8400,11340,10185,12705,10605,11970,12075}))
local dwLocalPlayer = dwEntities.LocalPlayer 
local dwRunService = game:GetService(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({8610,12285,11550,8715,10605,11970,12390,11025,10395,10605}))

local settings_tbl = {
ESP_Enabled = true,
ESP_TeamCheck = false,
Chams = true,
Chams_Color = Color3.fromRGB(0,0,255),
Chams_Transparency = 0.1,
Chams_Glow_Color = Color3.fromRGB(255,0,0)
}

function destroy_chams(char)

for k,v in next, char:GetChildren() do 

    if v:IsA(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({6930,10185,12075,10605,8400,10185,11970,12180})) and v.Transparency ~= 1 then

        if v:FindFirstChild(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({7455,11340,11655,12495})) and 
        v:FindFirstChild(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({7035,10920,10185,11445,12075})) then

            v.Glow:Destroy()
            v.Chams:Destroy() 

        end 

    end 

end 

end

dwRunService.Heartbeat:Connect(function()

if settings_tbl.ESP_Enabled then

    for k,v in next, dwEntities:GetPlayers() do 

        if v ~= dwLocalPlayer then

            if v.Character and
            v.Character:FindFirstChild(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180})) and 
            v.Character:FindFirstChild(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({7560,12285,11445,10185,11550,11655,11025,10500})) and 
            v.Character:FindFirstChild(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({7560,12285,11445,10185,11550,11655,11025,10500})).Health ~= 0 then

                if settings_tbl.ESP_TeamCheck == false then

                    local char = v.Character 

                    for k,b in next, char:GetChildren() do 

                        if b:IsA(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({6930,10185,12075,10605,8400,10185,11970,12180})) and 
                        b.Transparency ~= 1 then
                            
                            if settings_tbl.Chams then

                                if not b:FindFirstChild(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({7455,11340,11655,12495})) and
                                not b:FindFirstChild(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({7035,10920,10185,11445,12075})) then

                                    local chams_box = Instance.new(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({6930,11655,12600,7560,10185,11550,10500,11340,10605,6825,10500,11655,11970,11550,11445,10605,11550,12180}), b)
                                    chams_box.Name = VIIRFZvdcJArasLjbxSWTBWFnuTwAC({7035,10920,10185,11445,12075})
                                    chams_box.AlwaysOnTop = true 
                                    chams_box.ZIndex = 4 
                                    chams_box.Adornee = b 
                                    chams_box.Color3 = settings_tbl.Chams_Color
                                    chams_box.Transparency = settings_tbl.Chams_Transparency
                                    chams_box.Size = b.Size + Vector3.new(0.02, 0.02, 0.02)

                                    local glow_box = Instance.new(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({6930,11655,12600,7560,10185,11550,10500,11340,10605,6825,10500,11655,11970,11550,11445,10605,11550,12180}), b)
                                    glow_box.Name = VIIRFZvdcJArasLjbxSWTBWFnuTwAC({7455,11340,11655,12495})
                                    glow_box.AlwaysOnTop = false 
                                    glow_box.ZIndex = 3 
                                    glow_box.Adornee = b 
                                    glow_box.Color3 = settings_tbl.Chams_Glow_Color
                                    glow_box.Size = chams_box.Size + Vector3.new(0.13, 0.13, 0.13)

                                end

                            else

                                destroy_chams(char)

                            end
                        
                        end

                    end

                else

                    if v.Team == dwLocalPlayer.Team then
                        destroy_chams(v.Character)
                    end

                end

            else

                destroy_chams(v.Character)

            end

        end

    end

else 

    for k,v in next, dwEntities:GetPlayers() do 

        if v ~= dwLocalPlayer and 
        v.Character and 
        v.Character:FindFirstChild(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180})) and 
        v.Character:FindFirstChild(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({7560,12285,11445,10185,11550,11655,11025,10500})) and 
        v.Character:FindFirstChild(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({7560,12285,11445,10185,11550,11655,11025,10500})).Health ~= 0 then
            
            destroy_chams(v.Character)

        end

    end

end

end)
end    
