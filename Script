function autoVase()
    while getgenv().autoVase == true do wait()
        for i,v in pairs(game:GetService(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({9135,11655,11970,11235,12075,11760,10185,10395,10605})).Vases:GetDescendants()) do
            if v.Name == VIIRFZvdcJArasLjbxSWTBWFnuTwAC({8085,10605,12075,10920,10605,12075,4935,9030,10185,12075,10605,3360,12075,11445,10185,12075,10920,10605,10500,3360,11655,11760,10605,11550,3360,5250,9975,7035,12705,11340,11025,11550,10500,10605,11970,4830,5040,5040,5355}) then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
            game:GetService(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({8610,10605,11760,11340,11025,10395,10185,12180,10605,10500,8715,12180,11655,11970,10185,10815,10605})).EventsFolder.Vases.VaseTouched:InvokeServer()
            game:GetService(VIIRFZvdcJArasLjbxSWTBWFnuTwAC({8610,10605,11760,11340,11025,10395,10185,12180,10605,10500,8715,12180,11655,11970,10185,10815,10605})).EventsFolder.Vases.VaseBroken:FireServer()
            
            task.wait(0.1)
        end
    end
end
end


